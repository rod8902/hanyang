      <XMLDocumentationFileName>$(IntDir)%%ProjectName%%\</XMLDocumentationFileName>
      <WarningLevel>Level3</WarningLevel>
      <DebugInformationFormat>ProgramDatabase</DebugInformationFormat>
    </ClCompile>
    <Link>
      <AdditionalDependencies>nddscppd.lib;nddscd.lib;nddscored.lib</AdditionalDependencies>
      <OutputFile>$(SolutionDir)bin\x64\$(Configuration)-VS2010\%%IDLFileBaseName%%_%%ProjectName%%.exe</OutputFile>
